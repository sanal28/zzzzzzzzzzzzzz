	margin: 0px 0px 0px 0px;
	
}

.space{
	padding: 10px;
}

.supportdiv{
	text-align: right;
	padding: 5px;
	margin: 0px;
}

.supportdiv span{
	font-family: 'Lato', sans-serif;
	font-size: 0.9em;
	font-weight: bold;
	color: #000;
	padding: 5px;
	margin: 0px;
}


.supportdiv a{
	font-family: 'Lato', sans-serif;
	font-size: 0.9em;
	font-weight: bold;
	color: #000;
	padding: 5px;
	margin: 0px;
}
.loginerror{
	font-family: 'Lato', sans-serif;
	font-size: 0.7em;
	font-weight: bold;
	color: #d00000;
	padding: 50px 0px;
	margin: 0px;
	text-align: center;
}
.btnmaindiv span{
	float: left;
	width: 18px;
	height: 18px;
