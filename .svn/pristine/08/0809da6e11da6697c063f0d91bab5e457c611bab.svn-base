.faquestiondiv{
	
	padding: 0px;
	margin: 10px 50px 20px 50px;
	border-bottom: 1px solid #c8c1ae;
}

.faquestiondiv h3{
	font-family: 'Lato', sans-serif;
	font-size: 1.2em;
	font-weight: bold;
	color: #7f9855;
	padding: 0px;
	margin: 5px 0px 0px 5px;

}

