/* Menu class */

.menumaindiv {
float: right;
  margin: 1px auto 0px auto;
  max-width: 600px;
}

.menumaindiv .currentitem{
background-color: #9c90da;
	color: #fff;
}



.menumaindiv ul {
  font-size: 0;
  list-style-type: none;
	padding: 0;
	margin: 0;
}
.menumaindiv ul li {
  font-family: 'Lato', sans-serif;
  font-size: 12px;
  font-weight: 400;
  color: #b3b0c2;
  display: inline-block;
  padding: 15px 20px;
  position: relative;
	text-transform: uppercase;
}
.menumaindiv ul li ul {
  display: none;
	z-index: 1000;
}
.menumaindiv ul li:hover {
  cursor: pointer;
  /*background-color: #9c90da;*/
	color: #fff;
}
.menumaindiv ul li:hover ul {
  display: block;
  margin-top: 15px;
  width: 200px;
  left: 0;
  position: absolute;
}
.menumaindiv ul li:hover ul li {
  display: block;
  background-color: #9c90da;
	color: #fff;
}
.menumaindiv ul li:hover ul li span {
  float: right;
  color: #f9f9f9;
  background-color: #f03861;
  padding: 2px 5px;
  text-align: center;
  font-size: 0.8rem;
  border-radius: 3px;
}
.menumaindiv ul li:hover ul li:hover {
  background-color: #6d60b0;
}
.menumaindiv ul li:hover ul li:hover span {
  background-color: #9c90da;
}
/* Menu ends here*/

